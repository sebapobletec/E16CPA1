1. CREATE DATABASE call_list;
2. CREATE TABLE users (id SERIAL PRIMARY KEY, firs_name VARCHAR(64), email VARCHAR(64));
3. INSERT INTO users VALUES (1, 'Carlos','carlos@gmail.com');
4. INSERT INTO users VALUES (2, 'Laura','laura@gmail.com');
5. CREATE TABLE calls (id SERIAL PRIMARY KEY, phone VARCHAR(15), date DATE, user_id INTEGER REFERENCES users(id));
6. ALTER TABLE users ADD COLUMN last_name varchar(64);
7. UPDATE users SET last_name = 'Perez' WHERE id=1;
8. UPDATE users SET last_name = 'Gomez' WHERE id=2;
